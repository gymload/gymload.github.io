$gymload_builder $mol_page
	title \Builder
	Day* $gymload_builder_day
		title <= day_title* \Day
		day_index <= day_index* 0
	body /
		<= WeekCount_labeler $mol_labeler
			title @ \Week Count
			Content <= WeekCount $mol_number
				value? <=> week_count? 10
		<= DayCount_labeler $mol_labeler
			title @ \Day Count
			Content <= DayCount $mol_number
				value? <=> day_count? 3
		<= DumbbellWeighs $gymload_builder_weights
		<= Weeks $mol_deck
			items <= week_items /$mol_view
