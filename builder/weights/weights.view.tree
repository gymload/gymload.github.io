$gymload_builder_weights $mol_view
	type \dumbbell
	Item* $mol_view
		sub /
			<= Labeler* $mol_labeler
				title <= row_title* \
				Content <= Value* $mol_number
					precision 0.1
					value? <=> row_value*? 0
			<= RemoveButton* $mol_button_major
				title \x
				click?event <=> row_remove*? null
	sub /
		<= AddButton $mol_button_major
			title \+
			click?event <= add? null
